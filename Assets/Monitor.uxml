<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <engine:VisualElement name="monitor" style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; -unity-background-scale-mode: scale-to-fit;" />
    <engine:DropdownField name="selector" data-source-type="Nsm.SourceSelector, Assembly-CSharp" style="position: absolute; top: 10%; left: 10%; max-width: 500px; right: 10%;">
        <Bindings>
            <engine:DataBinding property="choices" data-source-path="SourceList" binding-mode="ToTarget" />
        </Bindings>
    </engine:DropdownField>
</engine:UXML>
